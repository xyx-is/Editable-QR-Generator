    "message": "Generated image size is too large. Too large size image may use a huge amount of memory and make the browser non-responsive.\nAre you sure to continue?",
    "description": "Text of the confirm message box which appears when an user trys to make large width image"
  },

  "errorDescriptionLabel": {
    "message": "Error: $error$",
    "description": "Label of error",
    "placeholders": {
      "error": {
        "content": "$1",
        "example": "Invalid value"
      }
    }
  },
  "errorUserCanceledLargeImage": {
    "message": "Large size image generation canceled. Please use smaller size of margin and cell size.",
