  },

  "statsVersionLabel": {
    "message": "Version: $version$",
    "description": "Label description for the version in the statistics div",
    "placeholders": {
      "version": {
        "content": "$1",
        "example": "2"
      }
    }
  },
  "statsVersionCellSizeTitle": {
    "message": "$cellSize$ cells",
    "description": "Title description for the version in the statistics div",
    "placeholders": {
      "cellSize": {
        "content": "$1",
        "example": "25"
      }
    }
  },
  "statsSizeLabel": {
    "message": "Size: $cellSize$ cells = $pxSize$ px",
    "description": "Label description for the size in the statistics div",
    "placeholders": {
      "cellSize": {
        "content": "$1",
        "example": "25"
      },
      "pxSize": {
        "content": "$2",
        "example": "132"
      }
    }
  },
  "statsSizeTitle": {
    "message": "($cellSize$ cells + 2 * $margin$ margin cells) * $scale$ px/cell",
    "description": "Title description for the size in the statistics div",
    "placeholders": {
      "cellSize": {
        "content": "$1",
        "example": "25"
      },
      "margin": {
        "content": "$2",
        "example": "4"
      },
      "scale": {
        "content": "$3",
        "example": "4"
      }
    }
  },
  "statsMaskLabel": {
    "message": "Mask: Pattern $mask$",
    "description": "Label description for the mask in the statistics div",
    "placeholders": {
      "mask": {
        "content": "$1",
        "example": "2"
      }
    }
  },
  "statsEccLabel": {
    "message": "Ecc: $ecc$",
    "description": "Label description for the ecc in the statistics div",
    "placeholders": {
      "ecc": {
        "content": "$1",
        "example": "M"
      }
    }
  },
  "statsModesLabel": {
    "message": "Modes: $modes$",
    "description": "Label description for the modes in the statistics div",
    "placeholders": {
      "modes": {
        "content": "$1",
        "example": "[BYTE]"
      }
    }
  }
